{
    "rewrites": [
        {
            "source": "/((?!api|assets|favicon.ico|robots.txt|sitemap.xml).*)",
            "destination": "/index.html"
        }
    ],
    "headers": [
        {
            "source": "/assets/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        }
    ]
}